{
	"name": "Copy_SalesLT_CustomersV2",
	"properties": {
		"activities": [
			{
				"name": "CopyFromMSQLtoASQL",
				"type": "Copy",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"source": {
						"type": "SqlSource",
						"sqlReaderQuery": "SELECT CustomerID, NameStyle, Title, FirstName, MiddleName, LastName, Suffix,\nCompanyName, SalesPerson, EmailAddress, Phone, PasswordHash, PasswordSalt,\nCAST(rowguid as NVARCHAR(50)) AS rowguid, ModifiedDate FROM SalesLT.Customer"
					},
					"sink": {
						"type": "SqlSink",
						"writeBatchSize": 10000,
						"preCopyScript": {
							"value": "@{pipeline().parameters.SinkPreCopyScript}",
							"type": "Expression"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_MSQL_SalesLT_Customer",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ASQL_SalesLT_Customer",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "dataflow1",
				"type": "ExecuteDataFlow",
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"typeProperties": {
					"dataflow": {
						"referenceName": "dataflow1",
						"type": "DataFlowReference"
					},
					"staging": {
						"linkedService": {
							"referenceName": "LS_ADF_StorageForDataFlows",
							"type": "LinkedServiceReference"
						},
						"folderPath": "df-staging"
					},
					"compute": {
						"computeType": "General",
						"dataTransformationUnits": 4,
						"coreCount": 8,
						"numberOfNodes": 0
					}
				}
			}
		],
		"parameters": {
			"SourceQuery": {
				"type": "String",
				"defaultValue": "SELECT CustomerID, NameStyle, Title, FirstName, MiddleName, LastName, Suffix, CompanyName, SalesPerson, EmailAddress, Phone, PasswordHash, PasswordSalt, CAST(rowguid as NVARCHAR(50)) AS rowguid, ModifiedDate FROM SalesLT.Customer"
			},
			"SinkPreCopyScript": {
				"type": "String",
				"defaultValue": "TRUNCATE TABLE [AWLTSRC].[SalesLT_Customer]"
			}
		},
		"folder": {
			"name": "Lab 02"
		}
	}
}