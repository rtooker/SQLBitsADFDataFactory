{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "SQLBitsADFDataFactory"
		},
		"LS_ASQL_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL_Staging'"
		},
		"LS_MSQL_AdventureWorksLT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_MSQL_AdventureWorksLT'"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://sqlbitsdlsv1.azuredatalakestore.net/webhdfs/v1"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "56f45b33-b6ef-44ac-828a-d47edc7c733f"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "c1f1036e-0dba-4a3a-845c-deaccf21f354"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "SQLBitsADF"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/LS_ADLS_sqlbitsdlsv1')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDataLakeStore",
				"typeProperties": {
					"dataLakeStoreUri": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_dataLakeStoreUri')]",
					"tenant": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_tenant')]",
					"subscriptionId": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_subscriptionId')]",
					"resourceGroupName": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_resourceGroupName')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS_ASQL_Staging')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": "[parameters('LS_ASQL_Staging_connectionString')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/SQLBitsSelfHostedIntegrationRuntime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/LS_MSQL_AdventureWorksLT')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "SqlServer",
				"typeProperties": {
					"connectionString": "[parameters('LS_MSQL_AdventureWorksLT_connectionString')]"
				},
				"connectVia": {
					"referenceName": "SQLBitsSelfHostedIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/SQLBitsSelfHostedIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/DS_MSQL_SalesLT_Customer')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "LS_MSQL_AdventureWorksLT",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "SqlServerTable",
				"typeProperties": {
					"tableName": "[concat('[', 'SalesLT].[Customer]')]"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/LS_MSQL_AdventureWorksLT')]"
			]
		}
	]
}