{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory Name",
			"defaultValue": "SQLBitsADFDataFactory"
		},
		"LS_ASQL_Staging_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_ASQL_Staging'"
		},
		"LS_MSQL_AdventureWorksLT_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_MSQL_AdventureWorksLT'"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_dataLakeStoreUri": {
			"type": "string",
			"defaultValue": "https://sqlbitsdlsv1.azuredatalakestore.net/webhdfs/v1"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "56f45b33-b6ef-44ac-828a-d47edc7c733f"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_subscriptionId": {
			"type": "string",
			"defaultValue": "c1f1036e-0dba-4a3a-845c-deaccf21f354"
		},
		"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_resourceGroupName": {
			"type": "string",
			"defaultValue": "SQLBitsADF"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas Token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ASQL_Staging_connectionString": {
						"value": "[parameters('LS_ASQL_Staging_connectionString')]"
					},
					"LS_MSQL_AdventureWorksLT_connectionString": {
						"value": "[parameters('LS_MSQL_AdventureWorksLT_connectionString')]"
					},
					"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_dataLakeStoreUri": {
						"value": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_dataLakeStoreUri')]"
					},
					"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_tenant": {
						"value": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_tenant')]"
					},
					"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_subscriptionId": {
						"value": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_subscriptionId')]"
					},
					"LS_ADLS_sqlbitsdlsv1_properties_typeProperties_resourceGroupName": {
						"value": "[parameters('LS_ADLS_sqlbitsdlsv1_properties_typeProperties_resourceGroupName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}